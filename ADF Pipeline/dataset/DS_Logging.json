{
	"name": "DS_Logging",
	"properties": {
		"linkedServiceName": {
			"referenceName": "LS_ADLS_Parametrized",
			"type": "LinkedServiceReference",
			"parameters": {
				"account_name": "@pipeline().parameters.account_name",
				"container_name": "@pipeline().parameters.log_container"
			}
		},
		"parameters": {
			"log_container": {
				"type": "string"
			},
			"log_path": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "AzureBlobFSLocation",
				"fileName": "pipeline-log-@utcNow().csv",
				"folderPath": {
					"value": "pipeline-logs",
					"type": "Expression"
				},
				"fileSystem": "activity-logs"
			},
			"columnDelimiter": ",",
			"escapeChar": "\\",
			"firstRowAsHeader": true,
			"quoteChar": "\""
		},
		"schema": []
	}
}